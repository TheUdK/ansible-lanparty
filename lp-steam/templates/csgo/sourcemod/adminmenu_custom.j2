"Commands"
{
  "Match Admin"
  {
    "Pause"
    {
      "admin"   "warmod"
      "cmd"     "sm_pause"
      "execute" "server"
    }
    "Unpause"
    {
      "admin"   "warmod"
      "cmd"     "sm_unpause"
      "execute" "server"
    }
    "Swap Teams"
    {
      "admin"   "warmod"
      "cmd"     "aswap"
      "execute" "player"
    }
    "Bullet Markers"
    {
      "admin"   "warmod"
      "cmd"     "sv_showimpacts @1"
      "execute" "server"
      "1"
      {
        "type"    "onoff"
        "title"   "Bullet Markers:"
      }
    }
    "Knife on 3"
    {
      "admin"   "warmod"
      "cmd"     "ko3"
      "execute" "server"
    }
    "Restart & Set Money"
    {
      "admin"   "warmod"
      "cmd"     "mp_startmoney @1; mp_restartgame 2"
      "execute" "server"
      "1"
      {
        "type"    "list"
        "title"   "Startmoney"
        "1"       "3000"
        "1."      "$3000"
        "2"       "5000"
        "2."      "$5000"
        "3"       "7000"
        "3."      "$7000"
        "4"       "10000"
        "4."      "$10000"
        "5"       "800"
        "5."      "$800"
      }
    }
  }
}
